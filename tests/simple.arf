:arf
module simple
require "io"
---

func rr main
        > argc:Int
        > argv:{String}
        < status:Int
        ---
        write 1 "hello world" 11

func rr write
        > fileDescriptor:UInt
        > buffer:Uint8
        > length:UInt
        < status:Int
        ---
        asm "syscall" "sdf"
                > "a" 1
                > "d" fileDescriptor
                > "S" buffer
                > "d" length
                < "a" status
                % "rcx" "r11" "memory"

